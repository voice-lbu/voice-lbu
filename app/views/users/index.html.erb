<%= link_to 'Opprett ny bruker', new_user_path, class: 'btn btn-primary pull-right' %>

<h1>Brukere</h1>

<table class="table">
  <thead>
    <tr>
      <th>Navn</th>
      <th>E-post</th>
      <th>Telefon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, user %></td>
        <td><%= user.email %></td>
        <td><%= user.phone %></td>
      </tr>
    <% end %>
  </tbody>
</table>
